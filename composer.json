{
  "name": "simutrans-potal/simutrans-potal",
  "type": "project",
  "description": "simutrans potal",
  "keywords": [
    "simutrans"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "abraham/twitteroauth": "^6.1",
    "berkayk/onesignal-laravel": "^2.0",
    "cebe/markdown": "^1.2",
    "doctrine/dbal": "^3.6",
    "ezyang/htmlpurifier": "^4.16",
    "google/cloud-recaptcha-enterprise": "^1.4",
    "guzzlehttp/guzzle": "^7.8",
    "htmlmin/htmlmin": "dev-master#347d577",
    "laravel-notification-channels/discord": "^1",
    "laravel/framework": "^10.24",
    "laravel/sanctum": "^3.3",
    "laravel/tinker": "^2.8",
    "laravel/ui": "^4.2",
    "marvinlabs/laravel-discord-logger": "^1.4",
    "spatie/flysystem-dropbox": "^3",
    "spatie/laravel-backup": "^8.3",
    "spatie/laravel-feed": "^4.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.9",
    "barryvdh/laravel-ide-helper": "^2.13",
    "brianium/paratest": "^7.2",
    "fakerphp/faker": "^1.23",
    "filp/whoops": "^2.15",
    "laravel/dusk": "^7.11",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.25",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^7.9",
    "nunomaduro/larastan": "^2.6",
    "phpunit/phpunit": "^10.3",
    "spatie/laravel-ignition": "^2.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "8.1.20"
    },
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "test": [
      "@php artisan test && @php artisan dusk"
    ],
    "pint": [
      "./vendor/bin/pint"
    ],
    "pint:check": [
      "./vendor/bin/pint --test"
    ],
    "stan": [
      "./vendor/bin/phpstan analyse --memory-limit=1G"
    ],
    "stan:clear": [
      "./vendor/bin/phpstan clear-result-cache"
    ],
    "stan:base": [
      "./vendor/bin/phpstan analyse --memory-limit=1G --generate-baseline --allow-empty-baseline"
    ],
    "all": [
      "@pint",
      "@stan",
      "@test"
    ]
  }
}