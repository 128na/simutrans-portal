    "cross-env": "^5.2.1",
    "jquery": "^3.5.1",
    "laravel-mix": "^4.1.4",
    "luxon": "^1.25.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.16.1",
    "resolve-url-loader": "^2.3.1",
    "sass": "^1.28.0",
    "sass-loader": "^7.3.1",
    "vue": "^2.6.12",
    "vue-clipboard2": "^0.3.1",
    "vue-i18n": "^8.22.1",
    "vue-router": "^3.4.8",
