{
  "name": "simutrans-potal/simutrans-potal",
  "type": "project",
  "description": "simutrans potal",
  "keywords": [
    "simutrans"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "abordage/laravel-html-min": "^0.2.1",
    "abraham/twitteroauth": "^7",
    "berkayk/onesignal-laravel": "^2.1.0",
    "cebe/markdown": "^1.2.1",
    "ezyang/htmlpurifier": "^4.17",
    "google/cloud-recaptcha-enterprise": "^1.13.0",
    "guzzlehttp/guzzle": "^7.8.1",
    "laravel-notification-channels/discord": "^1.6",
    "laravel/fortify": "^1.21.5",
    "laravel/framework": "^11.15.0",
    "laravel/sanctum": "^4.0.2",
    "laravel/tinker": "^2.9",
    "marvinlabs/laravel-discord-logger": "^1.4.2",
    "potibm/phluesky": "^0.3.0",
    "spatie/flysystem-dropbox": "^3.0.1",
    "spatie/laravel-backup": "^8.8.1",
    "spatie/laravel-feed": "^4.4",
    "spatie/laravel-sitemap": "^7.2.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.13.5",
    "barryvdh/laravel-ide-helper": "^3.1",
    "dg/bypass-finals": "^1.8",
    "fakerphp/faker": "^1.23.1",
    "filp/whoops": "^2.15.4",
    "larastan/larastan": "^2.9.8",
    "laravel/dusk": "^8.2.1",
    "laravel/pint": "^1.16.2",
    "mockery/mockery": "^1.6.12",
    "nunomaduro/collision": "^8.1.1",
    "phpunit/phpunit": "^10.5.27",
    "rector/rector": "^1.2.0",
    "spatie/laravel-ignition": "^2.8.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "8.3.2"
    },
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "all": [
        "composer dump-autoload",
        "@php artisan ide-helper:generate",
        "@php artisan ide-helper:models -WMR",
        "@rector",
        "@stan",
        "@pint"
    ],
    "pint": [
        "@php ./vendor/bin/pint"
    ],
    "pint:check": [
        "@php ./vendor/bin/pint --test"
    ],
    "stan": [
      "@php ./vendor/bin/phpstan analyse --memory-limit=2G"
    ],
    "rector": [
      "@php ./vendor/bin/rector --no-diffs"
    ]
  }
}
