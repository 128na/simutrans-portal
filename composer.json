{
  "name": "simutrans-potal/simutrans-potal",
  "type": "project",
  "description": "simutrans potal",
  "keywords": [
    "simutrans"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "abraham/twitteroauth": "^1.2",
    "cebe/markdown": "^1.2",
    "doctrine/dbal": "^3.0",
    "ezyang/htmlpurifier": "^4.13",
    "fideloper/proxy": "^4.0",
    "fruitcake/laravel-cors": "^2.0",
    "kreait/firebase-php": "^5.21",
    "laravel/framework": "^8.0",
    "laravel/passport": "^10.1",
    "laravel/socialite": "^5.2",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^2.0",
    "socialiteproviders/google": "^4.1",
    "socialiteproviders/twitter": "^4.1",
    "spatie/flysystem-dropbox": "^1.2",
    "spatie/laravel-backup": "^6.11",
    "spatie/laravel-feed": "^3.0",
    "tinify/tinify": "^1.5",
    "yarri/full-text-search-query-like": "^0.2.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.3",
    "barryvdh/laravel-ide-helper": "^2.10",
    "brianium/paratest": "^6.2",
    "fakerphp/faker": "^1.9.1",
    "filp/whoops": "^2.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "phpunit/php-code-coverage": "^9.2",
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "7.4.15"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "test": [
      "@php artisan test --parallel --processes=2"
    ],
    "coverage_local": [
      "phpunit --coverage-text=coverage/coverage.txt --coverage-html=coverage/html"
    ],
    "coverage_ci": [
      "phpunit --coverage-html=coverage"
    ]
  }
}
