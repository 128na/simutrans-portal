{
  "name": "simutrans-potal/simutrans-potal",
  "type": "project",
  "description": "simutrans potal",
  "keywords": [
    "simutrans"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "abordage/laravel-html-min": "^0.2.1",
    "abraham/twitteroauth": "^7",
    "berkayk/onesignal-laravel": "^2.1.0",
    "cebe/markdown": "^1.2.1",
    "doctrine/dbal": ">=4.0.1",
    "ezyang/htmlpurifier": "^4.17",
    "google/cloud-recaptcha-enterprise": "^1.10.0",
    "guzzlehttp/guzzle": "^7.8.1",
    "laravel-notification-channels/discord": "^1.6",
    "laravel/fortify": "^1.21.1",
    "laravel/framework": "^11.1.1",
    "laravel/sanctum": "^4.0.1",
    "laravel/tinker": "^2.9",
    "marvinlabs/laravel-discord-logger": "^1.4.2",
    "potibm/phluesky": "^0.3.0",
    "spatie/flysystem-dropbox": "^3.0.1",
    "spatie/laravel-backup": "^8.6.0",
    "spatie/laravel-feed": "^4.4"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23.1",
    "nunomaduro/collision": "^8.1.1",
    "barryvdh/laravel-debugbar": "^3.12.2",
    "barryvdh/laravel-ide-helper": "^3",
    "brianium/paratest": "^7.4.3",
    "filp/whoops": "^2.15.4",
    "larastan/larastan": "^2.9.2",
    "laravel/dusk": "^8.1",
    "laravel/pint": "^1.15.0",
    "laravel/sail": "^1.29.1",
    "mockery/mockery": "^1.6.11",
    "phpunit/phpunit": ">=10.5.15",
    "rector/rector": "^1.0.3",
    "spatie/laravel-ignition": "^2.5.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "8.2.11"
    },
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "all": [
        "composer dump-autoload",
        "@php artisan ide-helper:generate",
        "@rector",
        "@stan",
        "@pint"
    ],
    "pint": [
        "@php ./vendor/bin/pint"
    ],
    "pint:check": [
        "@php ./vendor/bin/pint --test"
    ],
    "stan": [
      "@php ./vendor/bin/phpstan analyse --memory-limit=2G"
    ],
    "rector": [
      "@php ./vendor/bin/rector --no-diffs"
    ]
  }
}
