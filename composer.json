{
  "name": "simutrans-potal/simutrans-potal",
  "type": "project",
  "description": "simutrans potal",
  "keywords": [
    "simutrans"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0.2",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/framework": "^9.0",
    "laravel/sanctum": "^3.0",
    "laravel/tinker": "^2.7",
    "abraham/twitteroauth": "^3",
    "cebe/markdown": "^1.2",
    "doctrine/dbal": "^3.0",
    "ezyang/htmlpurifier": "^4.13",
    "fideloper/proxy": "^4.0",
    "laravel/slack-notification-channel": "^2.4",
    "laravel/ui": "^3",
    "spatie/flysystem-dropbox": "^2.0",
    "spatie/laravel-backup": "^8.1",
    "spatie/laravel-feed": "^4.2",
    "tinify/tinify": "^1.5",
    "yarri/full-text-search-query-like": "^0.2.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.5.10",
    "spatie/laravel-ignition": "^1.0",
    "barryvdh/laravel-debugbar": "^3.3",
    "barryvdh/laravel-ide-helper": "^2.10",
    "brianium/paratest": "^6.2",
    "filp/whoops": "^2.0",
    "laravel/dusk": "^6.25",
    "phpunit/php-code-coverage": "^9.2",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "8.0.25"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "test": [
      "@php artisan test && @php artisan dusk"
    ],
    "cs-check": [
      "vendor/bin/phpcs --standard=ruleset.xml"
    ],
    "cs": [
      "vendor/bin/phpcbf --standard=ruleset.xml"
    ],
    "coverage_local": [
      "phpunit --coverage-text=coverage/coverage.txt --coverage-html=coverage/html"
    ],
    "coverage_ci": [
      "phpunit --coverage-html=coverage"
    ]
  }
}
