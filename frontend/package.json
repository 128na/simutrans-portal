{
  "name": "frontend",
  "version": "0.0.1",
  "description": "Simutrans Addon Portal front app",
  "productName": "Simutrans Addon Portal",
  "author": "128na <simutrans.128na@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "fix": "eslint --fix --ext .js,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build && npm run spa:copy",
    "spa:copy": "node ./deployment/spa",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:8080 \"cypress open --e2e\"",
    "test:e2e:ci": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:8080 \"cypress run --e2e\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component",
    "ncu:check": "npx ncu",
    "ncu:update": "npx ncu -u",
    "nr:s": "node ./deployment/sourcemap"
  },
  "dependencies": {
    "@quasar/extras": "^1",
    "@sentry/tracing": "^7.43.0",
    "@sentry/vue": "^7.43.0",
    "apexcharts": "^3",
    "axios": "^1",
    "core-js": "^3",
    "luxon": "^3",
    "markdown-it": "^13",
    "pinia": "^2",
    "quasar": "^2",
    "sanitize-html": "^2",
    "vue": "^3",
    "vue-router": "^4",
    "vue3-apexcharts": "^1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7",
    "@newrelic/publish-sourcemap": "^5",
    "@quasar/app": "^3",
    "@quasar/quasar-app-extension-testing": "^2",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.0",
    "cypress": "^12.8.1",
    "dotenv": "^16",
    "eslint": "^8",
    "eslint-config-airbnb-base": "^15",
    "eslint-plugin-chai-friendly": "^0.7",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2",
    "eslint-plugin-vue": "^9",
    "eslint-webpack-plugin": "^4",
    "fs-extra": "^11",
    "fs-readdir-recursive": "^1",
    "playwright-webkit": "^1",
    "workbox-webpack-plugin": "^6.6.0"
  },
  "browserslist": [
    "last 15 versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
